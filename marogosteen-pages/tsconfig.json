{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "@config": [
        "config.ts"
      ],
      "@components/*": [
        "components/*"
      ],
      "@contents/*": [
        "contents/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@libs/*": [
        "libs/*"
      ]
    }
  }
}
