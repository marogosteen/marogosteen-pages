{
	"name": "marogosteen-pages",
	"dockerComposeFile": "compose.yaml",
	"workspaceFolder": "/workspaces/marogosteen-pages",
	"service": "app",
	"features": {
		"ghcr.io/devcontainers-extra/features/cloudflare-wrangler:1": {},
		"ghcr.io/devcontainers-contrib/features/typescript:2": {}
	},
	"postCreateCommand": "cd marogosteen-pages&&yarn install",
	"customizations": {
		"vscode": {
			"extensions": [
				"eamodio.gitlens",
				"astro-build.astro-vscode",
				"svelte.svelte-vscode",
				"GitHub.copilot",
				"GitHub.copilot-chat",
				"bradlc.vscode-tailwindcss"
			]
		}
	}
}
