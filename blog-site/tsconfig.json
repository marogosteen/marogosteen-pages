{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "@config": [
        "config.ts"
      ],
      "@components/*": [
        "components/*"
      ],
      "@contents/*": [
        "contents/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@pages/*": [
        "pages/*"
      ]
    }
  }
}